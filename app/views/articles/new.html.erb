<h1>Ananananana Article</h1>
<img src="http://read-static.tidal.com/wp-content/uploads/2017/03/10013108/Banana_12001-780x440.jpg"
     alt="Hasta Banana" title="pretentious banana">

<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
